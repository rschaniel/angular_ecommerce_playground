{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "prefetching-sample",
  "projects": {
    "prefetching-sample": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/prefetching-sample",
      "sourceRoot": "apps/prefetching-sample/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/prefetching-sample",
            "index": "apps/prefetching-sample/src/index.html",
            "main": "apps/prefetching-sample/src/main.ts",
            "polyfills": "apps/prefetching-sample/src/polyfills.ts",
            "tsConfig": "apps/prefetching-sample/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/prefetching-sample/src/favicon.ico",
              "apps/prefetching-sample/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/prefetching-sample/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/prefetching-sample/src/environments/environment.ts",
                  "with": "apps/prefetching-sample/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "prefetching-sample:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "prefetching-sample:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "prefetching-sample:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/prefetching-sample/src/test.ts",
            "polyfills": "apps/prefetching-sample/src/polyfills.ts",
            "tsConfig": "apps/prefetching-sample/tsconfig.spec.json",
            "karmaConfig": "apps/prefetching-sample/karma.conf.js",
            "assets": [
              "apps/prefetching-sample/src/favicon.ico",
              "apps/prefetching-sample/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/prefetching-sample/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/prefetching-sample/tsconfig.app.json",
              "apps/prefetching-sample/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "prefetching-sample:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "prefetching-sample:serve:production"
            }
          }
        }
      }
    },
    "prefetching-sample-e2e": {
      "root": "apps/prefetching-sample-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/prefetching-sample-e2e/protractor.conf.js",
            "devServerTarget": "prefetching-sample:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "prefetching-sample:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/prefetching-sample-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "orders": {
      "projectType": "application",
      "root": "apps/orders",
      "sourceRoot": "apps/orders/src",
      "prefix": "prefetching-sample",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/orders",
            "index": "apps/orders/src/index.html",
            "main": "apps/orders/src/main.ts",
            "polyfills": "apps/orders/src/polyfills.ts",
            "tsConfig": "apps/orders/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/orders/src/favicon.ico", "apps/orders/src/assets"],
            "styles": ["apps/orders/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/orders/src/environments/environment.ts",
                  "with": "apps/orders/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "orders:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "orders:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "orders:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/orders/src/**/*.ts",
              "apps/orders/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/orders"],
          "options": {
            "jestConfig": "apps/orders/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "orders-e2e": {
      "root": "apps/orders-e2e",
      "sourceRoot": "apps/orders-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/orders-e2e/cypress.json",
            "tsConfig": "apps/orders-e2e/tsconfig.e2e.json",
            "devServerTarget": "orders:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "orders:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/orders-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
